    "tsup": "^8.0.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^1.1.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "directories": {
